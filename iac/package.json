{
  "name": "wave-catalog",
  "main": "index.ts",
  "packageManager": "pnpm@9.13.2",
  "scripts": {
    "pre": "GOOGLE_APPLICATION_CREDENTIALS=./credentials.json pulumi pre --yes",
    "up": "GOOGLE_APPLICATION_CREDENTIALS=./credentials.json pulumi up --yes",
    "config": "GOOGLE_APPLICATION_CREDENTIALS=./credentials.json pulumi config",
    "destroy": "GOOGLE_APPLICATION_CREDENTIALS=./credentials.json pulumi destroy --continue-on-error --yes",
    "stack": "GOOGLE_APPLICATION_CREDENTIALS=./credentials.json pulumi stack",
    "refresh": "GOOGLE_APPLICATION_CREDENTIALS=./credentials.json pulumi refresh --skip-preview",
    "cancel": "GOOGLE_APPLICATION_CREDENTIALS=./credentials.json pulumi cancel",
    "login": "GOOGLE_APPLICATION_CREDENTIALS=./credentials.json pulumi login"
  },
  "devDependencies": {
    "@types/node": "^18",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@pulumi/docker": "^4.7.0",
    "@pulumi/gcp": "^8.0.0",
    "@pulumi/pulumi": "^3.113.0",
    "@pulumi/random": "^4.18.2"
  }
}
